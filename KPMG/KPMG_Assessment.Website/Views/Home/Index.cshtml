@{
    ViewData["Title"] = "Home Page";
}
<script src="https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.7.7/xlsx.core.min.js"></script>

<div class="container">
    <div class="row">
        <h2>Import Transactions</h2>
        <form>
            <div class="col-md-6">
                <div class="form-group">
                    <div class="col-md-10">
                        <input type="file" id="excelfile" />

                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <div class="col-md-10">
                        <input type="button" id="viewfile" class="btn-primary" value="Import Transactions" onclick="ImportFile()" />
                    </div>
                </div>
            </div>
        </form>
    </div>

</div>

<div class="container">
    <div class="row" id="divResponse">
        <div class="col-md-12">
            <span id="processLog" class="small"></span>
        </div>
    </div>
    <div>
        <table id="exceltable"></table>
    </div>
</div>
<div class="container" id="divTransactions">

</div>



